<?xml version="0.0" ?>
<robot name="urdf_robot">
  <link name="baseLink">
    <contact>
      <lateral_friction value="3.5"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="2.0"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
	    <sphere radius="0.5"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
        <specular rgb="11 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	    <sphere radius="0.5"/>
      </geometry>
    </collision>
  </link>
</robot>

